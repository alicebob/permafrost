{
    "manifest_version": 2,
    "name": "Permafrost",
    "version": "2015.7.0",
    "description": "A local ajax.googleapis.com mirror",
    "author": "Harmen",
    "background": {
        "scripts": ["background.js"]
    },
    "homepage_url": "https://github.com/alicebob/permafrost",
    "minimum_chrome_version": "22.0",
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "http://ajax.googleapis.com/ajax/libs/*",
        "https://ajax.googleapis.com/ajax/libs/*"
    ],
    "web_accessible_resources": [
        "pool/*"
    ]
}
