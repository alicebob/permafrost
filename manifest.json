{
    "manifest_version": 2,
    "name": "Permafrost",
    "version": "0.1",
    "description": "Local ajax.googleapis.com",
    "author": "Harmen",
    "background": {
        "page": "background.html"
    },
    "homepage_url": "https://github.com/alicebob/permafrost",
    "minimum_chrome_version": "22.0",
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "http://ajax.googleapis.com/ajax/libs/*",
        "https://ajax.googleapis.com/ajax/libs/*"
    ],
    "web_accessible_resources": [
        "pool/*"
    ]
}
